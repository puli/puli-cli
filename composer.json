{
    "name": "puli/cli",
    "description": "A Command Line Interface (CLI) for managing Puli projects.",
    "homepage": "http://puli.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Bernhard Schussek",
            "email": "bschussek@gmail.com"
        }
    ],
    "require": {
        "php": "^5.3.9|^7.0",
        "padraic/humbug_get_contents": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Puli\\Cli\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Puli\\Cli\\Tests\\": "tests/",
            "Puli\\Repository\\Tests\\": "vendor/puli/repository/tests/",
            "Puli\\Manager\\Tests\\": "vendor/puli/manager/tests/",
            "Puli\\Discovery\\Tests\\": "vendor/puli/discovery/tests/"
        }
    },
    "bin": ["bin/puli"],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "support": {
        "issues": "https://github.com/puli/issues/issues"
    }
}
